Proceso Punto5
	Definir nombre Como Caracter;
	Definir mascota Como Caracter;
	Definir edad Como Entero;
	Definir dueno Como Caracter;
	
	Escribir "Nombre de la mascota";
	leer nombre;
	
	Escribir "¿Qué tipo de animal es?";
	leer mascota;
	
	Escribir "¿Cuántos años tiene?";
	leer edad;
	
	Escribir "Nombre completo del dueño";
	leer dueno;
	
	Limpiar Pantalla;
	Escribir nombre, " es un ", mascota, " el cual tiene ", edad, " años de edad, y ", dueno, " actualmente es su dueño(a) ";
FinProceso
